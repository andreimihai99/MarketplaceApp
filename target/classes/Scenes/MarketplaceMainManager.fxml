<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="468.0" prefWidth="659.0" style="-fx-background-color: #f4a460;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MarketplaceMainManagerController">
   <children>
      <Label layoutX="52.0" layoutY="23.0" prefHeight="40.0" prefWidth="257.0" text="Welcome, Manager!">
         <font>
            <Font name="Segoe UI Light Italic" size="28.0" />
         </font>
      </Label>
      <Label layoutX="203.0" layoutY="79.0" text="What would you like to do?" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Segoe UI Light" size="17.0" />
         </font>
      </Label>
      <Label layoutX="98.0" layoutY="139.0" text="Check product quantities and prices in stock " textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Segoe UI Light" size="15.0" />
         </font>
      </Label>
      <Button layoutX="431.0" layoutY="134.0" mnemonicParsing="false" onAction="#displayProductsButtonAction" prefHeight="31.0" prefWidth="137.0" style="-fx-background-color: #ffdead;" text="Display products">
         <font>
            <Font name="Segoe UI Light" size="15.0" />
         </font>
      </Button>
      <Label layoutX="52.0" layoutY="179.0" prefHeight="21.0" prefWidth="186.0" text="Add a product to the market" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Segoe UI Light" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="newProdNameField" layoutX="268.0" layoutY="175.0" prefHeight="29.0" prefWidth="137.0" promptText="new product name" style="-fx-background-color: #ffdead;">
         <font>
            <Font name="Segoe UI Light" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="newProdQuantityField" layoutX="268.0" layoutY="211.0" prefHeight="29.0" prefWidth="137.0" promptText="new product quantity" style="-fx-background-color: #ffdead;">
         <font>
            <Font name="Segoe UI Light" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="newProdPriceField" layoutX="268.0" layoutY="248.0" prefHeight="29.0" prefWidth="137.0" promptText="new product price" style="-fx-background-color: #ffdead;">
         <font>
            <Font name="Segoe UI Light" size="13.0" />
         </font>
      </TextField>
      <Button layoutX="431.0" layoutY="210.0" mnemonicParsing="false" onAction="#addNewProductButtonAction" prefHeight="31.0" prefWidth="137.0" style="-fx-background-color: #ffdead;" text="Add new product">
         <font>
            <Font name="Segoe UI Light" size="15.0" />
         </font>
      </Button>
      <Label layoutX="52.0" layoutY="302.0" prefHeight="21.0" prefWidth="216.0" text="Delete a product from the market" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Segoe UI Light" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="deleteProdName" layoutX="268.0" layoutY="298.0" prefHeight="29.0" prefWidth="137.0" promptText="product name" style="-fx-background-color: #ffdead;">
         <font>
            <Font name="Segoe UI Light" size="13.0" />
         </font>
      </TextField>
      <Button layoutX="431.0" layoutY="297.0" mnemonicParsing="false" onAction="#deleteProductButtonAction" prefHeight="31.0" prefWidth="137.0" style="-fx-background-color: #ffdead;" text="Delete product">
         <font>
            <Font name="Segoe UI Light" size="15.0" />
         </font>
      </Button>
      <AnchorPane layoutX="57.0" layoutY="348.0" prefHeight="82.0" prefWidth="559.0" style="-fx-background-color: #CD853F;">
         <children>
            <Label layoutX="60.0" layoutY="30.0" prefHeight="21.0" prefWidth="160.0" text="Modify price or quantity" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Segoe UI Light" size="15.0" />
               </font>
            </Label>
            <Button layoutX="269.0" layoutY="25.0" mnemonicParsing="false" onAction="#modifyProductButtonAction" prefHeight="31.0" prefWidth="216.0" style="-fx-background-color: #ffdead;" text="Go to modify product page">
               <font>
                  <Font name="Segoe UI Light" size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
